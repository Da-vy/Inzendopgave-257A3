<Window x:Class="Inzendopgave_257A3.Windows.RankingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inzendopgave_257A3.Windows"
        mc:Ignorable="d"
        Title="RankingsWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="DataGrid1_Rankings" HorizontalAlignment="Left" Height="300" Margin="10,10,0,0" VerticalAlignment="Top" Width="600" AutoGenerateColumns="False" ColumnWidth="*" BorderBrush="LightGray">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Speler" Binding="{Binding Speler}" />
                <DataGridTextColumn Header="Team" Binding="{Binding Team}" />
                <DataGridTextColumn Header="Punten" Binding="{Binding Punten}" />
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
